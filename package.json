    "@commitlint/travis-cli": "7.2.1",
    "@ember/optional-features": "0.6.4",
    "@types/ember": "2.8.15",
    "@types/ember-qunit": "3.4.3",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.5.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "~4.2.3",
    "ember-cli-fastboot": "^1.1.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-template-lint": "1.0.0-beta.2",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-sinon": "1.0.1",
    "ember-sinon-qunit": "2.1.0",
    "ember-source": "https://s3.amazonaws.com/builds.emberjs.com/canary/shas/a662910a5ce72ce84ba8867a5abfcf1b30158dcf.tgz",
    "ember-source-channel-url": "1.1.0",
    "ember-try": "1.1.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "greenkeeper-lockfile": "1.14.0",
    "husky": "1.2.0",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.0"
  },
  "optionalDependencies": {
    "@mike-north/js-lib-renovate-config": "^0.0.1",
    "@mike-north/js-lib-semantic-release-config": "^0.0.0-development",
    "travis-deploy-once": "^5.0.9",
    "semantic-release": "^15.9.12"
  },
  "keywords": [
    "ember-addon",
    "intercom",
    "chat"
  ],
  "repository": "https://github.com/mike-north/ember-intercom-io",
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (http://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "dependencies": {
    "ember-cli-babel": "^6.16.0",
    "ember-cli-htmlbars": "^3.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
